{
    "name": "chubbyphp/chubbyphp-slim-psr15",
    "description": "A slim psr15 adapter for middleware and request handler.",
    "keywords": ["chubbyphp", "slim", "psr-15", "adapter", "middleware", "request-handler"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0.1",
        "psr/http-server-middleware": "^1.0.1"
    },
    "require-dev": {
        "chubbyphp/chubbyphp-mock": "^1.4.2",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phploc/phploc": "^4.0.1|^5.0",
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "^6.5.14|^7.5.16|^8.4.2"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\SlimPsr15\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Chubbyphp\\Tests\\SlimPsr15\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
